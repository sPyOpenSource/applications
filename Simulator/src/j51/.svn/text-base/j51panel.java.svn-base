/**
 *
 * 8051 Simulator base panel.
 *  
 * @author Mario Viara
 * @version 1.00
 */
class J51Panel extends JPanel
{
	protected MCS51 cpu;
	private String title;

	J51Panel(String title)
	{
		this(title,true);
	}
	
	J51Panel(String title,boolean box)
	{
		super(new GridBagLayout());
		this.title = title;
		if (box)
		{
			if (title == null)
				JFactory.setBox(this);
			else
				JFactory.setTitle(this,title);
		}
	}

	public String getTitle()
	{
		return title;
	}

	public void setCpu(MCS51 cpu)
	{
		this.cpu = cpu;
	}

	public void setEmulation(boolean mode)
	{
	}

	public void update(boolean force)
	{
	}
}
